block project-page

section.overlay
    button#closeModal.close-button
        span St√§ng projekt
    section.projectPage#ppContent
        div.projectHeader.bgc-grey-dark
            .projectImgBox
                img.img-lg#projectHeader
            .projectTextBox
                h1#projectTitle
                p.ingress#projectArea
                p.citat#projectCitat
        section.s-col_m-row.bgc-grey-dark.pdg-top-50.pdg-btm-50.mrg-btm-80.resp-type
            article.s-col_m-row_item
                h2#projectClientTitle
                p.em.pdg-btm
                  a.link-yellow#projectClient(target="_blank")
                p#projectKunden_desc
            article.s-col_m-row_item
                h2#projectOmProjektet
                p.em.pdg-btm#projectOmProjektetEm
                p#projectOmProjektetDesc
            article.s-col_m-row_item.mrg-btm-40
                h2#projectArbetet
                p.em.pdg-btm#projectArbetetEm
                p#projectArbetetDesc
        section.divider-section.text-dark.pdg-top-20
            h2.text-dark.mrg-top-40.mrg-btm#projectDividerTitle_one
            p.text-dark.ingress#projectDividerDesc_one
            img.img-mid.mrg-top-80#projectDividerImg_one
        section.s-col_m-row-left.pdg-sides-10p.pdg-btm-50            
            article
