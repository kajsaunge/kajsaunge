        h2.text-dark.mrg-top-40.mrg-btm= generic.education
        p.text-dark.ingress.mrg-btm-30= generic.educationText
        .utb-block
            p.utb-category= generic.educationTypeProfessional
                -var educationList = []
                -for(var key in education)
                    -educationList.push(education[key])
                each education in educationList
                    .utb-item
                        p.text-333_90p.em.mrg-btm--5= education.date
                        h5.font-bodycopy.caps.mrg-btm--5= education.degree
                        p.em.mrg-top-5
                            a.link-yellow(href= education.schoolLonk target="_blank")= education.school
            p.utb-category= generic.educationTypeOnline
                -var educationOnlineList = []
                -for(var key in educationOnline)
                    -educationOnlineList.push(educationOnline[key])
                each educationOnline in educationOnlineList
                    .utb-item
                        p.text-333_90p.em.mrg-btm--5= educationOnline.date
                        h5.font-bodycopy.caps.mrg-btm--5= educationOnline.degree
                        p.em.mrg-top-5
                            a.link-yellow(href= educationOnline.schoolLink target="_blank")= educationOnline.school