block cv
section.s-col
        img.img-linework.pdg-sides-5p.mrg-top-80(src='../img/kajsa-unge_linework.png')
section.site-container-col.mrg-top.mrg-btm-40
        h1#cv.text-dark.m-mrg-top-40.mrg-btm= generic.menuCv
        p.text-dark.ingress.mrg-btm-30= generic.cvText
        .cv-block.mrg-btm-50.pdg
            -var experienceList = []
            -for(var key in experience)
                -experienceList.push(experience[key])
            each experience in experienceList
                .cv-item
                    p.text-333_90p.em.mrg-btm--5= experience.date
                    h3.caps= experience.title
                    a.link-pine.em(href= experience.companyLinks)= experience.company
                    p.small-hide= experience.experienceDesc
                    span.bold= experience.experienceTakeaway
